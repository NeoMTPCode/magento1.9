    $jq(document).ready(function () {

        $jq(".tab_content_electronics").hide();
        $jq(".tab_content_electronics:nth-child(2)").show();

        $jq("ul.tabs-electronics li").click(function () {
            $jq("ul.tabs-electronics li").removeClass("active");
