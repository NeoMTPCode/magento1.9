<script type="text/javascript">

    $jq(document).ready(function () {

        $jq(".tab_content_wine").hide();
        $jq(".tab_content_wine:nth-child(2)").show();

        $jq("ul.tabs-wine li").click(function () {
            $jq("ul.tabs-wine li").removeClass("active");
            $jq(this).addClass("active");
            $jq(".tab_content_wine").hide();
            var activeTab = $jq(this).find("p").attr('class');
            $jq("#" + activeTab).fadeIn();
        });
    });

</script> 
