<script type="text/javascript">

    $jq(document).ready(function () {

        $jq(".tab_content_kitchenware").hide();
        $jq(".tab_content_kitchenware:nth-child(2)").show();

        $jq("ul.tabs-kitchenware li").click(function () {
            $jq("ul.tabs-kitchenware li").removeClass("active");
            $jq(this).addClass("active");
            $jq(".tab_content_kitchenware").hide();
            var activeTab = $jq(this).find("p").attr('class');
            $jq("#" + activeTab).fadeIn();
        });
    });

</script> 
