<script type="text/javascript">

    $jq(document).ready(function () {

        $jq(".tab_content_fashion").hide();
        $jq(".tab_content_fashion:nth-child(2)").show();

        $jq("ul.tabs-fashion li").click(function () {
            $jq("ul.tabs-fashion li").removeClass("active");
            $jq(this).addClass("active");
            $jq(".tab_content_fashion").hide();
            var activeTab = $jq(this).find("p").attr('class');
            $jq("#" + activeTab).fadeIn();
        });
    });

</script> 
