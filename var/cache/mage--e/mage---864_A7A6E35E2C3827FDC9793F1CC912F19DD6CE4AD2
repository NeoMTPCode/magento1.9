<script type="text/javascript">

    $jq(document).ready(function () {

        $jq(".tab_content_tools").hide();
        $jq(".tab_content_tools:nth-child(2)").show();

        $jq("ul.tabs-tools li").click(function () {
            $jq("ul.tabs-tools li").removeClass("active");
            $jq(this).addClass("active");
            $jq(".tab_content_tools").hide();
            var activeTab = $jq(this).find("p").attr('class');
            $jq("#" + activeTab).fadeIn();
        });
    });

</script> 
